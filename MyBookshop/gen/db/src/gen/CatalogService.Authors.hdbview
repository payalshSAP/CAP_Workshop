VIEW CatalogService_Authors AS SELECT
  Authors_0.createdAt,
  Authors_0.createdBy,
  Authors_0.modifiedAt,
  Authors_0.modifiedBy,
  Authors_0.ID,
  Authors_0.name,
  Authors_0.dateOfBirth,
  Authors_0.nationality
FROM my_bookshop_Authors AS Authors_0
WITH ASSOCIATIONS (
  MANY TO MANY JOIN CatalogService_Books AS books ON (books.author_ID = ID)
)